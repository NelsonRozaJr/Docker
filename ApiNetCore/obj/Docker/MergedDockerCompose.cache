networks:
  back-end: {}
services:
  db:
    container_name: mysql
    environment:
      MYSQL_ROOT_PASSWORD: '12345678'
      bind-address: 0.0.0.0
    image: mysql
    networks:
      back-end: {}
  webapi:
    build:
      context: D:\Documents\Visual Studio 2019\Projects\Docker\ApiNetCore
      dockerfile: Dockerfile
    container_name: catalog-api
    depends_on:
    - db
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    networks:
      back-end: {}
    ports:
    - published: 3000
      target: 80
    restart: on-failure
version: '3.4'