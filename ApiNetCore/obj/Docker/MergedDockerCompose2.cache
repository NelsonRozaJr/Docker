services:
  apinetcore:
    build:
      context: D:\Documents\Visual Studio 2019\Projects\Docker\ApiNetCore
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ApiNetCore
      target: base
    container_name: ApiNetCore
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: apinetcore:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "/app/bin/Debug/netcoreapp3.1/ApiNetCore.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 3000
      target: 80
    - published: 3001
      target: 443
    tty: true
    volumes:
    - /d/Documents/Visual Studio 2019/Projects/Docker/ApiNetCore:/app:rw
    - /c/Users/Administrator/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/Administrator/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/Administrator/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Microsoft/Xamarin/NuGet:/root/.nuget/fallbackpackages:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - /c/Users/Administrator/.nuget/packages:/root/.nuget/packages:ro
    - /d/Documents/Visual Studio 2019/Projects/Docker/ApiNetCore:/src:rw
version: '3.4'